{
  "name": "boil-it",
  "displayName": "Boil It",
  "description": "This extension helps seasoned developers avoid writing redundant boilerplate code for sophisticated Python backends using an AI agent.",
  "version": "0.0.1",
  "publisher": "AGirdhar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anubhavgirdhar1/boil-it"
  },
  "homepage": "https://github.com/anubhavgirdhar1/boil-it",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": ["Other"],
  "extensionKind": ["workspace"],
  "capabilities": {
    "virtualWorkspaces": false
  },
  "activationEvents": [
    "onView:boilItView"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "boilItContainer",
          "title": "Boil It",
          "icon": "media/boil-it.svg"
        }
      ]
    },
    "views": {
      "boilItContainer": [
        {
          "id": "boilItView",
          "name": "Boilerplate Generator",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
